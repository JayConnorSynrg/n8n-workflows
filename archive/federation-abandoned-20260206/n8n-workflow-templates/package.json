{
  "name": "@federation/n8n-workflow-templates",
  "version": "1.0.0",
  "description": "Composable n8n workflow template system for Federation platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "deploy-department": "ts-node scripts/deploy-department.ts",
    "convert-workflow": "ts-node scripts/convert-to-templates.ts",
    "validate-templates": "ts-node scripts/validate-templates.ts",
    "dry-run": "ts-node scripts/deploy-department.ts --dry-run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "n8n",
    "workflow",
    "templates",
    "federation",
    "multi-tenant"
  ],
  "author": "Federation Platform Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/federation"
  }
}
